{
  "name": "More-Recipes",
  "version": "1.0.0",
  "description": "A recipe application that enables users share insightful recipes.",
  "main": "index.js",
  "engines": {
    "node": "8.9.1",
    "npm": "5.5.1"
  },
  "scripts": {
    "build": "babel ./server/ -d dist --copy-files",
    "build:prod": "webpack -p --env production",
    "dev-server": "webpack-dev-server",
    "start-webpack": "webpack --watch",
    "start:web": "webpack-dashboard -- webpack-dev-server",
    "start:dev": "npm run apidoc && nodemon --exec babel-node ./server/bin/www.js",
    "apidoc": "apidoc -i server/routes -o public/apidoc",
    "start": "npm run build && npm run apidoc && node ./dist/bin/www.js && npm run build:client",
    "heroku-postbuild": "npm run migrate",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "test-jest": "jest",
    "nyc-local": "cross-env NODE_ENV=test npm run test-migrate && cross-env NODE_ENV=test npm run test",
    "test-local": "cross-env NODE_ENV=test npm run test-migrate && cross-env NODE_ENV=test mocha --compilers js:babel-core/register server/__test__/*.spec.js --timeout 50000 --exit",
    "test": "nyc mocha server/__test__/*.spec.js",
    "test-migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "coveralls": "nyc npm run nyc-local && nyc report --reporter=text-lcov | coveralls",
    "dev": "nodemon --exec babel-node ./server/bin/www.js",
    "client": "npm run start --prefix client",
    "conc": "concurrently \"npm run client\" \"npm run dev\"",
    "build:client": "npm run build --prefix client"
  },
  "nyc": {
    "require": [
      "babel-register"
    ]
  },
  "apidoc": {
    "name": "More-Recipes API documentation",
    "template": {
      "forceLanguage": "en"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Billmike/More-Recipes.git"
  },
  "keywords": [],
  "author": "Bill Michael <billmike1994@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Billmike/More-Recipes/issues"
  },
  "homepage": "https://github.com/Billmike/More-Recipes#readme",
  "devDependencies": {
    "apidoc": "^0.15.1",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "expect": "^21.2.1",
    "html-webpack-plugin": "^2.30.1",
    "jsdoc": "^3.5.5",
    "live-server": "^1.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "react-hot-loader": "^3.1.1",
    "standard": "^10.0.3",
    "standard-loader": "^6.0.1",
    "webpack-dashboard": "^1.0.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "apidoc": "^0.15.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "dot-env": "0.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "jest": "^21.2.1",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "nodemailer": "^4.3.1",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.1.1",
    "react-addons-shallow-compare": "^15.6.2",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.1.1",
    "react-modal": "^3.1.4",
    "react-redux": "^5.0.6",
    "react-responsive-carousel": "^3.1.28",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.17",
    "redux": "^3.7.2",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.15.0",
    "sequelize-cli": "^3.0.0",
    "style-loader": "^0.19.0",
    "supertest": "^3.0.0",
    "webpack": "^2.2.0-rc.4"
  }
}
